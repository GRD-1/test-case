{
  "repository": "https://github.com/GRD-1/test-case-zftf",
  "name": "test-case-zftf",
  "author": "GRD-1",
  "engines": {
    "node": ">=20.14.0"
  },
  "scripts": {
    "run-typescript": "ts-node-transpile-only",
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:only-errors": "eslint --quiet \"{src,test}/**/*.ts\" --fix",
    "start:prod": "nest start -e 'node -r dotenv/config --es-module-specifier-resolution=node --experimental-modules'",
    "start:dev": "nest start -e 'node -r dotenv/config --es-module-specifier-resolution=node --experimental-modules' --debug --watch",
    "generate-keys": "keys/generate-rs256-keys.sh",
    "prisma:gen": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:validate": "prisma validate",
    "migration:create": "prisma migrate dev --create-only --name ",
    "migration:up": "prisma migrate deploy",
    "migration:reset": "prisma migrate reset",
    "test": "NODE_OPTIONS='--loader ts-node/esm' jest --runInBand --colors --config jest.config.json",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@prisma/client": "^5.15.0",
    "@prisma/extension-read-replicas": "^0.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^9.0.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.10",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^15.1.0",
    "prettier": "^2.8.1",
    "prisma": "^5.15.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.3.2"
  }
}
